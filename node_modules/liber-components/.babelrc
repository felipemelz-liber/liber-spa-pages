{
  "env": {
    "production": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "useBuiltIns": "usage",
            "corejs": 2
          }
        ],
        "@babel/preset-react"
      ],
      "plugins": [
        ["module-resolver",
          {
            "root": [
              "./src"
            ]
          }
        ],
        "styled-components",
        "transform-class-properties",
        "@babel/plugin-proposal-object-rest-spread",
        "transform-react-jsx"
      ],
      "ignore": [
        "**/__tests__/*.js",
        "**/__tests__/*.jsx",
        "**/__test__/*.js",
        "**/__test__/*.jsx",
        "**/*.dev.js",
        "**/*.dev.jsx"
      ]
    },
    "development": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "useBuiltIns": "usage",
            "corejs": 2
          }
        ],
        "@babel/preset-react"
      ],
      "plugins": [
        "styled-components",
        "transform-class-properties",
        "@babel/plugin-proposal-object-rest-spread",
        "transform-react-jsx"
      ],
      "ignore": [
        "**/__tests__/*.js",
        "**/__tests__/*.jsx",
        "**/__test__/*.js",
        "**/__test__/*.jsx"
      ]
    },
    "test": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "useBuiltIns": "usage",
            "corejs": 2
          }
        ],
        "@babel/preset-react"
      ],
      "plugins": [
        "styled-components",
        "transform-class-properties",
        "@babel/plugin-proposal-object-rest-spread",
        "transform-react-jsx"
      ]
    }
  }
}